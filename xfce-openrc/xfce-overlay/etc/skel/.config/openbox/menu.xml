		<menu id="System" label="System">
			<item label="Avahi Zeroconf Browser">
				<action name="Execute">
					<execute>/usr/bin/avahi-discover</execute>
				</action>
			</item>
			<item label="Bulk Rename">
				<action name="Execute">
					<execute>/usr/lib/Thunar/ThunarBulkRename</execute>
				</action>
			</item>
			<item label="GParted">
				<action name="Execute">
					<execute>/usr/bin/gparted_polkit</execute>
				</action>
			</item>
			<item label="Htop">
				<action name="Execute">
					<execute>xterm -e 'htop'</execute>
				</action>
			</item>
			<item label="Manage Printing">
				<action name="Execute">
					<execute>/usr/bin/xdg-open http://localhost:631/</execute>
				</action>
			</item>
			<item label="Octopi CacheCleaner">
				<action name="Execute">
					<execute>octopi-cachecleaner</execute>
				</action>
