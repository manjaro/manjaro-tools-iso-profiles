			<item label="Task Manager">
				<action name="Execute">
					<execute>xfce4-taskmanager</execute>
				</action>
			</item>
			<item label="Thunar File Manager">
				<action name="Execute">
					<execute>thunar</execute>
				</action>
			</item>
