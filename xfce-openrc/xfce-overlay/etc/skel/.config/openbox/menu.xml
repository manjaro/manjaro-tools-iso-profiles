					<execute>xterm</execute>
				</action>
			</item>
		</menu>
		<separator/>
		<menu id="Configure" label="Configure">
			<item label="Change Desktop">
				<action name="Execute">
					<execute>nitrogen /usr/share/backgrounds</execute>
				</action>
			</item>
			<item label="Conky RC">
				<action name="Execute">
					<execute>mousepad ~/.conkyrc</execute>
				</action>
			</item>
			<item label="Tint2 Panel">
				<action name="Execute">
					<execute>mousepad ~/.config/tint2/tint2rc</execute>
				</action>
			</item>
			<menu id="Openbox" label="Openbox">
				<item label="Reconfigure Openbox">
					<action name="Execute">
						<execute>openbox --reconfigure</execute>
					</action>
				</item>
				<item label="Openbox Autostart">
					<action name="Execute">
						<execute>mousepad ~/.config/openbox/autostart</execute>
					</action>
				</item>
				<item label="Openbox RC">
					<action name="Execute">
						<execute>mousepad ~/.config/openbox/rc.xml</execute>
					</action>
				</item>
				<item label="Openbox Menu">
					<action name="Execute">
						<execute>mousepad ~/.config/openbox/menu.xml</execute>
					</action>
				</item>
				<separator/>
				<item label="Obmenu">
					<action name="Execute">
						<execute>obmenu</execute>
					</action>
				</item>
			</menu>
			<menu id="Obmenu-Generator" label="Obmenu-Generator">
				<item label="Menu Schema">
					<action name="Execute">
						<execute>mousepad ~/.config/obmenu-generator/schema.pl</execute>
					</action>
				</item>
				<item label="Menu Config">
					<action name="Execute">
						<execute>mousepad ~/.config/obmenu-generator/config.pl</execute>
					</action>
				</item>
				<separator/>
				<item label="Generate a pipe menu">
					<action name="Execute">
						<execute>obmenu-generator -p</execute>
					</action>
				</item>
				<item label="Generate a static menu">
					<action name="Execute">
						<execute>obmenu-generator -s</execute>
					</action>
				</item>
				<item label="Generate a pipe menu with icons">
					<action name="Execute">
						<execute>obmenu-generator -p -i</execute>
					</action>
				</item>
				<item label="Generate a static menu with icons">
					<action name="Execute">
						<execute>obmenu-generator -s -i</execute>
					</action>
				</item>
				<separator/>
				<item label="Refresh Icon Set">
					<action name="Execute">
						<execute>obmenu-generator -d</execute>
					</action>
				</item>
			</menu>
		</menu>
		<separator/>
		<item label="Lock">
			<action name="Execute">
				<execute>light-locker-command -l</execute>
			</action>
		</item>
		<item label="Exit">
			<action name="Execute">
				<execute>cb-exit</execute>
			</action>
		</item>
